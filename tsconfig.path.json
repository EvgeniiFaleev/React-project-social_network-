{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@music": [
        "features/music"
      ],
      "@ui": [
        "ui"
      ],
      "@news/*": [
        "features/news/*"
      ],
      "@profile/*": [
        "features/profile/*"
      ],
      "@users/*": [
        "features/users/*"
      ],
      "@auth/*": [
        "features/authentication/*"
      ],
      "@dialogs/*": [
        "features/dialogs/*"
      ],
      "@images/*": [
        "ui/assets/images/*"
      ],
      "@icons": [
        "ui/assets/icons"
      ],
      "@api/*": [
        "api/*"
      ],
      "@socialAPI": [
        "api/socialAPI.ts"
      ],
      "@utils": [
        "utils"
      ],
      "@pages": [
        "src/pages"
      ],
      "@store": [
        "src/lib/store"
      ]
    }
  }
}